{
  "name": "PDF Navigator Widget",
  "id": "pdf_navigator_widget",
  "description": "A widget for navigating PDF documents with JSON annotation data",
  "version": "1.0.0",
  "client_script": "pdf-navigator-widget.js",
  "css": "pdf-navigator-widget.css",
  "html": "pdf-navigator-widget.html",
  "options": {
    "title": {
      "type": "string",
      "default": "PDF Field Navigator",
      "description": "Widget title"
    },
    "max_file_size": {
      "type": "number",
      "default": 10,
      "description": "Maximum file size in MB"
    }
  },
  "dependencies": {
    "angular": "1.8.3",
    "pdfjs": "2.11.338"
  }
}
